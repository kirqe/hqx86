<div class="row">
	<div class="middle-12 columns">
		<% @users.each do |user| %>
			<ul class="inline-list items">
				<li><%= user.id %>.</li>
				<li class="label alert">Admin</li>
				<li class="label success">Mod</li>
				<li class="label secondary">activated</li>
			  <li><%= user.username %></li>
			  <li><%= user.email %></li>
			  <li><%= link_to "show", "#" %></li>
			  <li><%= link_to "edit", "#" %></li>
			  <li><%= link_to "delete", "#" %></li>
			</ul>
		<% end %>
	</div>
</div>